<?xml version="1.0"?>
<package >
  <metadata>
    <id>XmlDoc2CmdletDoc</id>
    <version>$version$</version>
    <title>Redgate XmlDoc2CmdletDoc tool</title>
    <authors>Redgate</authors>
    <owners>Redgate</owners>
    <projectUrl>https://github.com/red-gate/XmlDoc2CmdletDoc</projectUrl>
    <license type="file">license\LICENSE.md</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>Generates help documentation for binary PowerShell modules from XML Doc comments.</summary>
    <description>Tool used to generate .dll-Help.xml help files for binary PowerShell modules from XML Doc comments. Licensed under the terms of the new 3-clause BSD license.</description>
    <language>en-us</language>
    <copyright>Copyright 2014-2019 Red Gate Software Ltd and other contributors.</copyright>
    <tags>PowerShell help xmldoc</tags>
    <developmentDependency>true</developmentDependency>
    <releaseNotes><![CDATA[$releaseNotes$]]></releaseNotes>
    <dependencies>
      <group targetFramework="net461"/>
      <group targetFramework="net472"/>
      <group targetFramework="net48"/>
      <group targetFramework="netcoreapp2.1"/>
      <group targetFramework="netcoreapp3.1"/>
      <group targetFramework="netstandard2.0"/>
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$configuration$\net461\publish\**" target="tools\net461" />
    <file src="bin\$configuration$\net472\publish\**" target="tools\net472" />
    <file src="bin\$configuration$\net48\publish\**" target="tools\net48" />
    <file src="bin\$configuration$\netcoreapp2.1\publish\**" target="tools\netcoreapp2.1" />
    <file src="bin\$configuration$\netcoreapp3.1\publish\**" target="tools\netcoreapp3.1" />
    <file src="build\**" target="build" />
    <file src="buildMultiTargeting\**" target="buildMultiTargeting" />
    <file src="lib\**" target="lib" />
    <file src="..\LICENSE.md" target="license" />
  </files>
</package>